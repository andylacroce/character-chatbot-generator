{
  "name": "character-chatbot-generator",
  "version": "0.5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "ci": "npm run lint -- --max-warnings=0 && npm run lint:md && npm run type-check && npm run test:coverage && npm run build",
    "vercel-build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:md": "markdownlint --config .markdownlint.json \"**/*.md\" --ignore \"node_modules/**\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --silent --coverageReporters=text --coverageReporters=text-summary",
    "type-check": "tsc --noEmit",
    "postinstall": "node ./scripts/fix-express-tsconfig.cjs",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "sanitize-filename": "^1.6.3",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@trendmicro/react-toggle-switch": "^0.5.7",
    "@typescript-eslint/type-utils": "^8.48.1",
    "@typescript-eslint/typescript-estree": "^8.50.0",
    "@typescript-eslint/utils": "^8.50.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/blob": "^2.0.0",
    "@vercel/speed-insights": "^1.3.1",
    "axios": "^1.13.2",
    "brace-expansion": "^4.0.1",
    "critters": "^0.0.25",
    "eslint-config-next": "^16.0.10",
    "express-rate-limit": "^8.2.1",
    "lru-cache": "^11.2.4",
    "minimatch": "^10.1.1",
    "next": "^16.0.10",
    "node-fetch": "^3.3.2",
    "openai": "^6.15.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-window": "^2.2.3",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "^16.0.10",
    "@rushstack/eslint-patch": "^1.15.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "autoprefixer": "^10.4.23",
    "babel-jest": "^30.2.0",
    "baseline-browser-mapping": "^2.9.8",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "express": "^5.2.1",
    "ipinfo": "^1.5.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "typescript": "^5"
    ,
    "markdownlint-cli": "^0.47.0"
  }
}
